{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.hisnak.com/v1",
    "NEXT_PUBLIC_STRIPE_PUBLIC_KEY": "@stripe_public_key"
  },
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  }
}